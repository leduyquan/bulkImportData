{
  "manifest_version": 2,
  "version": "0.1",
  "name": "Bulk Data Upload",
  "description": "LeapXPert Tool",
  "permissions": [
    "storage",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "contextMenus"
  ],
  "icons": {
    "16": "assets/img/icons/16.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  }
}